#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/qitaoweng/RTES/picar/DeepPicar-v3/web/client/node_modules/.pnpm/he@1.2.0/node_modules/he/bin/node_modules:/Users/qitaoweng/RTES/picar/DeepPicar-v3/web/client/node_modules/.pnpm/he@1.2.0/node_modules/he/node_modules:/Users/qitaoweng/RTES/picar/DeepPicar-v3/web/client/node_modules/.pnpm/he@1.2.0/node_modules:/Users/qitaoweng/RTES/picar/DeepPicar-v3/web/client/node_modules/.pnpm/node_modules:/Users/qitaoweng/RTES/picar/DeepPicar-v3/web/client/node_modules:/Users/qitaoweng/RTES/picar/DeepPicar-v3/web/node_modules:/Users/qitaoweng/RTES/picar/DeepPicar-v3/node_modules:/Users/qitaoweng/RTES/picar/node_modules:/Users/qitaoweng/RTES/node_modules:/Users/qitaoweng/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/qitaoweng/RTES/picar/DeepPicar-v3/web/client/node_modules/.pnpm/he@1.2.0/node_modules/he/bin/node_modules:/Users/qitaoweng/RTES/picar/DeepPicar-v3/web/client/node_modules/.pnpm/he@1.2.0/node_modules/he/node_modules:/Users/qitaoweng/RTES/picar/DeepPicar-v3/web/client/node_modules/.pnpm/he@1.2.0/node_modules:/Users/qitaoweng/RTES/picar/DeepPicar-v3/web/client/node_modules/.pnpm/node_modules:/Users/qitaoweng/RTES/picar/DeepPicar-v3/web/client/node_modules:/Users/qitaoweng/RTES/picar/DeepPicar-v3/web/node_modules:/Users/qitaoweng/RTES/picar/DeepPicar-v3/node_modules:/Users/qitaoweng/RTES/picar/node_modules:/Users/qitaoweng/RTES/node_modules:/Users/qitaoweng/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/he" "$@"
else
  exec node  "$basedir/../../bin/he" "$@"
fi
